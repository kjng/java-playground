public class Apples {

	public static void main(String[] args) {
		
		int[] kevin = {1, 2, 3, 4, 5};
		
		for (int num : kevin) {
			System.out.println(num);
		}
		
		change(kevin);
		
		for (int num2 : kevin) {
			System.out.println(num2);
		}
		
	}
	
	public static void change(int x[]) {
		
		for (int i = 0; i < x.length; i++) {
			x[i] += 5;
		}
		
	}

}
